{
    "name": "animethemes/animethemes-server",
    "type": "project",
    "description": "AnimeThemes.moe resource management & API",
    "keywords": [
        "anime",
        "animethemes"
    ],
    "homepage": "https://animethemes.moe/",
    "readme": "README.md",
    "license": "MIT",
    "support": {
        "email": "admin@animethemes.moe"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/AnimeThemes"
        },
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/animethemes"
        }
    ],
    "require": {
        "php": "^8.0",
        "babenkoivan/elastic-migrations": "^1.4",
        "babenkoivan/elastic-scout-driver": "^1.3",
        "babenkoivan/elastic-scout-driver-plus": "^2.0",
        "bensampo/laravel-enum": "^3.2",
        "bepsvpt/secure-headers": "^7.1",
        "cyrildewit/eloquent-viewable": "^6.0",
        "david-griffiths/nova-dark-theme": "^1.0",
        "devpartners/auditable-log": "^1.0",
        "doctrine/dbal": "^2.12.1",
        "enlightn/enlightn": "^1.18",
        "enlightn/enlightnpro": "^1.14",
        "fakerphp/faker": "^1.13",
        "fideloper/proxy": "^4.3",
        "fruitcake/laravel-cors": "^2.0.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminatech/nova-config": "^1.0",
        "inspheric/nova-url-field": "^1.4",
        "laravel-notification-channels/discord": "^1.3",
        "laravel/framework": "^8.12",
        "laravel/horizon": "^5.7",
        "laravel/jetstream": "^2.0",
        "laravel/nova": "~3.0",
        "laravel/sanctum": "^2.8",
        "laravel/scout": "^9.0",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "livewire/livewire": "^2.3.3",
        "olssonm/l5-zxcvbn": "^4.4",
        "owen-it/laravel-auditing": "^12.0",
        "sfelix-martins/json-exception-handler": "^2.3",
        "spatie/commonmark-highlighter": "^2.1",
        "spatie/db-dumper": "^3.0",
        "spatie/laravel-json-api-paginate": "^1.10",
        "staudenmeir/belongs-to-through": "^2.11.1"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "facade/ignition": "^2.5",
        "joshgaber/novaunit": "^2.2",
        "mockery/mockery": "^1.3.2",
        "nunomaduro/collision": "^5.1",
        "nunomaduro/larastan": "^0.7",
        "phpunit/phpunit": "^9.3",
        "predis/predis": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "ext-pcntl": "8.0",
            "ext-posix": "8.0"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi",
            "@php artisan nova:publish --ansi",
            "@php artisan telescope:publish --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://satis.laravel-enlightn.com"
        }
    ]
}
