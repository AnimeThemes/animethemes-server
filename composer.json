{
    "name": "animethemes/animethemes-server",
    "type": "project",
    "description": "AnimeThemes.moe resource management & API",
    "keywords": [
        "anime",
        "animethemes"
    ],
    "homepage": "https://animethemes.moe/",
    "readme": "README.md",
    "license": "MIT",
    "support": {
        "email": "admin@animethemes.moe"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/AnimeThemes"
        },
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/animethemes"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "babenkoivan/elastic-migrations": "^2.0",
        "babenkoivan/elastic-scout-driver-plus": "^3.0",
        "bensampo/laravel-enum": "^5.0",
        "bepsvpt/secure-headers": "^7.2",
        "cyrildewit/eloquent-viewable": "^6.1",
        "david-griffiths/nova-dark-theme": "^1.0",
        "devpartners/auditable-log": "^1.0",
        "enlightn/enlightn": "^1.25",
        "enlightn/enlightnpro": "^1.16",
        "fakerphp/faker": "^1.17",
        "guzzlehttp/guzzle": "^7.4",
        "illuminatech/nova-config": "^1.0",
        "inspheric/nova-url-field": "^1.4",
        "laravel-notification-channels/discord": "^1.3",
        "laravel/framework": "^9.2",
        "laravel/horizon": "^5.8",
        "laravel/jetstream": "^2.6",
        "laravel/nova": "^3.31",
        "laravel/sanctum": "^2.14.1",
        "laravel/scout": "^9.4",
        "laravel/telescope": "^4.7",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "olssonm/l5-zxcvbn": "^4.5",
        "owen-it/laravel-auditing": "^12.1",
        "spatie/commonmark-highlighter": "^3.0",
        "spatie/db-dumper": "^3.1.1",
        "spatie/laravel-validation-rules": "^3.2",
        "staudenmeir/belongs-to-through": "^2.12",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.4",
        "joshgaber/novaunit": "^2.3",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^1.0",
        "phpunit/phpunit": "^9.5.10",
        "predis/predis": "^1.1.9",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "ext-pcntl": "8.0",
            "ext-posix": "8.0"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan nova:publish --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://satis.laravel-enlightn.com"
        }
    ]
}
