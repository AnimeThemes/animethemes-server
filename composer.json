{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "babenkoivan/scout-elasticsearch-driver": "^4.2",
        "bensampo/laravel-enum": "^3.0",
        "darkaonline/l5-swagger": "^8.0",
        "david-griffiths/nova-dark-theme": "^1.0",
        "devpartners/auditable-log": "^1.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "inspheric/nova-url-field": "^1.4",
        "laravel/framework": "^8.0",
        "laravel/nova": "~3.0",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "olssonm/l5-zxcvbn": "^4.3",
        "owen-it/laravel-auditing": "^11.0",
        "paranarimasu/nova-nested-form": "dev-master",
        "spatie/laravel-resource-links": "^1.1",
        "staudenmeir/belongs-to-through": "^2.11"
    },
    "require-dev": {
        "facade/ignition": "^2.3.7",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/paranarimasu/laravel-nova-nested-form"
        }
    ]
}
